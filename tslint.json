{
	"defaultSeverity": "error",
	"extends": "tslint:recommended",
	"jsRules": {
		"max-line-length": [
			true,
			170
		],
	},
	"severity": "warning",
	"rules": {
		"semicolon": [
			true
		],
		"ter-indent": [
			false
		],
		"ter-arrow-parens": false,
		"array-bracket-spacing": false,
		"max-line-length": [
			true,
			170
		],
		"trailing-comma": [
			false
		],
		"ter-prefer-arrow-callback": [
			false
		],
		"variable-name": [
			false
		],
		"function-name": [
			false
		],
		"space-before-function-paren": false,
		"import-name": [
			false
		],
		"align": [
			false
		],
		"semicolor": [
			false
		],
		"radix": [
			false
		]
	},
	"rulesDirectory": []
}
